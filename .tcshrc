#!/bin/tcsh

set     red="%{\033[1;31m%}"
set   green="%{\033[0;32m%}"
set  yellow="%{\033[1;33m%}"
set    blue="%{\033[1;34m%}"
set magenta="%{\033[1;35m%}"
set    cyan="%{\033[1;36m%}"
set   white="%{\033[0;37m%}"
set     end="%{\033[0m%}"
if ( $USER == root ) then
	set usr="${red}%n"
else
	set usr="${green}%n"
endif
set prompt="${usr}${blue}@%m ${cyan}%~ ${white}[%h] ${yellow}%B%T%b${green}%%${end} "
#set prompt="%n@%m%B%~%b[%h]%B%T%b%#"
unset red green yellow blue magenta cyan white end usr

alias ll 'ls -Al'
